@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container">
    
    <div class="alert alert-primary" role="alert">
        Globbing demo
    </div>

    <form class="mb-2" method="post" id="currentForm">
        <div class="mt-2">
            <div class="col-sm-10">

                <button
                    type="submit"
                    id="btn-submit"
                    class="btn btn-primary">
                    Enter
                </button>

            </div>
        </div>
    </form>

    <div id="dump">
        
        @{
            if (Model.Builder is not null)
            {
                @Html.Raw(Model.Builder.ToString())
            }
        }
    </div>
</div>


